{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "importHelpers": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@base/*": [
        "src/platforms/common/*"
      ]
    }
  },
  "include": [
    "src"
  ],
  "files": [
    "src/resource.d.ts"
  ]
}
